                    width: .933333rem /* 70/75 */
                    height: .933333rem /* 70/75 */
                    border-radius 50%
            .name-wrapper
                flex: 1 0 0;
                font-size: .426667rem /* 32/75 */
                color: #323232;
                padding: .133333rem /* 10/75 */ 0 .133333rem /* 10/75 */ 0px;
                white-space: nowrap;
                .shop-name
                    overflow: hidden;
                    text-overflow: ellipsis;
                    white-space: nowrap;
                    display: inline-block;
                    max-width: 5.333333rem /* 400/75 */
                    vertical-align: middle;
                .tittle-icon
                    float right
                    margin-right .2rem /* 15/75 */
                    font-size .64rem /* 48/75 */
