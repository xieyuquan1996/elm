.Order
    position relative
    height 100%
    width 100%
    overflow auto
    .order-wrapper
        position relative
        width 100%
        box-sizing border-box
        padding-left .4rem /* 30/75 */
        border-top .026667rem /* 2/75 */ solid #d7d7d7
        border-bottom .026667rem /* 2/75 */ solid #d7d7d7
        background-color #fff
        .tittle-wrapper
            padding: 10px 0;
            position: relative;
            display: -ms-flexbox;
            display: -webkit-flex;
            display: flex;
            -webkit-flex-direction: row;
            -ms-flex-direction: row;
            flex-direction: row;
            .avatar-wrapper
                margin-right: .266667rem /* 20/75 */
                width: .933333rem /* 70/75 */
                height: .933333rem /* 70/75 */
