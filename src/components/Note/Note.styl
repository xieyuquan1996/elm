.Note
    position relative
    width 100%
    background-color rgb(255,255,255)
    .header-wrapper
        height 1.333333rem /* 100/75 */
        font-size .426667rem /* 32/75 */
        color #333
        .left-header,.right-header
            line-height 1.333333rem /* 100/75 */
            padding 0 .213333rem /* 16/75 */
        .left-header
            float left
        .right-header
            float right
    .footer-wrapper
        width 100%
        box-sizing border-box
        padding 0 .426667rem /* 32/75 */ .32rem /* 24/75 */
        .note-wrapper
            position relative
            width 100%
            .note-text
                width 100%
                height 2.333333rem /* 175/75 */
                box-sizing border-box
                padding .32rem /* 24/75 */
                border .026667rem /* 2/75 */ solid #D3D3D3
            .note-text-length
                position absolute
                right .32rem /* 24/75 */
                bottom .32rem /* 24/75 */
                font-size .32rem /* 24/75 */
                color #999
        .note-tittle
            font-size .373333rem /* 28/75 */
            margin .426667rem /* 32/75 */ 0
            color #333
        .note-list-wrapper
            position relative
            .note-parent
                display flex
                flex-wrap wrap
                .note-content
                    display inline-block
                    margin 0 .213333rem /* 16/75 */ 0.213333rem /* 16/75 */ 0
                    padding 0 .213333rem /* 16/75 */
                    border .026667rem /* 2/75 */ solid #D3D3D3
                    line-height .8rem /* 60/75 */
                    font-size .373333rem /* 28/75 */
                    color #333
